            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "disabled": true,
          "settings": {
            "picker_type": "button"
          }
        },
        "5e2d56c3-f781-4b82-9e93-f20467c86a38": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.nacelle.presale_info.value }}",
            "text_style": "presale"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "1f3d293d-e722-4e5f-b9ef-83bca814ee6b": {
          "type": "presale_policy_copy",
          "settings": {
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "price",
        "description",
        "variant_picker",
        "5e2d56c3-f781-4b82-9e93-f20467c86a38",
        "quantity_selector",
        "buy_buttons",
        "share",
        "1f3d293d-e722-4e5f-b9ef-83bca814ee6b"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
