{
  "sections": {
    "product-settings": {
      "type": "main-product-settings",
      "settings": {
        "alter_product_style": true,
        "product_text_color": "#ffffff",
        "product_nav_text_color": "#ffffff",
        "button_color": "#c27619",
        "nav_logo_white": true,
        "product_bk_color": "rgba(0,0,0,0)",
