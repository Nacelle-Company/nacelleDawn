{
  "sections": {
    "product-settings": {
      "type": "main-product-settings",
      "settings": {
        "alter_product_style": true,
        "product_text_color": "#ffffff",
        "product_nav_text_color": "#ffffff",
        "button_color": "#5899e1",
        "nav_logo_white": true,
        "product_bk_color": "linear-gradient(135deg, rgba(144, 39, 156, 1), rgba(225, 112, 81, 1) 50%, rgba(248, 229, 112, 1) 100%)",
        "product_bk_img_none": false,
        "product_bk_overlay": "",
        "product_main_img_bk": "",
        "product_bk_img_size": "100% auto",
        "product_bk_img_repeat": "repeat-y",
        "product_bk_img_attachment": "scroll"
      }
    },
    "main": {
      "type": "main-product_presale",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "description": {
          "type": "description",
          "settings": {
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "price",
        "description",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "image_ratio": "adapt",
        "show_secondary_image": false,
        "add_image_padding": false,
        "show_image_outline": true,
        "show_vendor": false,
        "show_rating": false
      }
    }
  },
  "order": [
    "product-settings",
    "main",
    "product-recommendations"
  ]
}