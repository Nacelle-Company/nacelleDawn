{
  "sections": {
    "product-settings": {
      "type": "main-product-settings",
      "settings": {
        "alter_product_style": true,
        "product_text_color": "#ffffff",
        "product_nav_text_color": "#ffffff",
        "button_color": "#21437b",
        "nav_logo_white": true,
        "product_bk_color": "rgba(0,0,0,0)",
        "product_bk_img_none": false,
        "product_background_image": "shopify:\/\/shop_images\/Sectaurs-Background-transparent_f2129ec8-4e33-4b3d-8842-2c3596ea0377.webp",
