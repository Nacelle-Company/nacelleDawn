          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        },
        "e5e02fe4-92e9-4965-bdf5-468dc96773cd": {
          "type": "text",
          "settings": {
            "text": "Pre-Order Policy: By purchasing the product above you have agreed to the pre-order terms and conditions:Terms & Conditions: Cancellations for pre-orders are honored within 72 hours of the original date of purchase. Pre-orders cannot be refunded after this 72 hour window.Pre-order products can only be returned upon receipt if the product is damaged on arrival.",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "price",
        "description",
        "variant_picker",
        "subtitle",
        "quantity_selector",
        "buy_buttons",
        "share",
        "e5e02fe4-92e9-4965-bdf5-468dc96773cd"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
